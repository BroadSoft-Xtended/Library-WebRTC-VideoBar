.bdsft-webrtc
  .videoBar.classes
    .table.fixed.collapse
      .cell.leftSpacer
      .cell.cell-selfView
        .selfViewHide.icon.fadeable
          a.icon-selfViewHide(href='', title='Disable Self View')
        .selfViewShow.icon.fadeable
          a.icon-selfViewShow(href='', title='Enable Self View')
      .cell.cell-dialpad
        .dialpadShow.icon.fadeable
          a.icon-dialpadShow(href='', title='Show Dialpad')
        .dialpadHide.icon.fadeable
          a.icon-dialpadHide(href='', title='Hide Dialpad')
      .cell.muteHolder
      .cell.cell-hold
        .hold.icon.fadeable
          a.icon-hold(href='', title='Hold Call')
        .resume.icon.fadeable
          a.icon-resume(href='', title='Resume Call')
      .cell.cell-hangup
        .hangup.icon.fadeable
          a.icon-hangup(href='', title='Hangup')
          .subtitle End Conference
      .cell.cell-transfer
        .transfer.icon.fadeable
          a.icon-transfer(href='', title='Transfer')
      .cell.timerHolder
      .cell.cell-settings
        .settings.icon.fadeable
          a.icon-settings(href='', title='Settings')
      .cell.fullscreenHolder
      .cell.rightSpacer

